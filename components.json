{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "default",
  "rsc": true,
  "tsx": true,
  "tailwind": {
    "config": "tailwind.config.js",
    "css": "src/app/globals.css",
    "baseColor": "slate",
    "cssVariables": true
  },
  "aliases": {
    "components": "@/components",
    "utils": "@/lib/utils"
  },
  "registries": {
    "@shadcn": {
      "url": "https://ui.shadcn.com/registry/{name}.json",
      "from": "shadcn-ui"
    },
    "@my-components": {
      "url": "https://my-components-registry.vercel.app/api/registry/{name}.json"
    }
  }
}